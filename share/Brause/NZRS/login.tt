<login>
  <clID>[% user %]</clID>
  <pw>[% pass %]</pw>
  [%~ IF newpw %]
  <newPW>[% newpw %]</newPW>
  [%~ END %]
  <options>
    <version>1.0</version> 
    <lang>en</lang>
  </options>
  <svcs> 
      <objURI>urn:ietf:params:xml:ns:domain-1.0</objURI> 
      <objURI>urn:ietf:params:xml:ns:contact-1.0</objURI> 
      <svcExtension>
        <extURI>urn:ietf:params:xml:ns:secDNS-1.1</extURI>
      </svcExtension>
  </svcs>
</login>
